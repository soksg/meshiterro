<h2>プロフィール画像</h2>
<%= form_with model: @user, url: user_path(@user.id), method: :patch  do |f| %>

  <label for="input_Name">Name</label>
  <%= f.text_field :name, autofocus: true, id: "input_Name", placeholder: "名前" %>

  <label for="input_Image">ProfileImage</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>

  <%= f.submit '変更を保存' %>

<% end %>

